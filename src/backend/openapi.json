{
  "openapi": "3.1.0",
  "info": {
    "title": "TikkaMasalAI Backend",
    "version": "0.1.0"
  },
  "paths": {
    "/metrics": {
      "get": {
        "summary": "Metrics",
        "description": "Endpoint that serves Prometheus metrics.",
        "operationId": "metrics_metrics_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          }
        }
      }
    },
    "/predict": {
      "post": {
        "tags": [
          "prediction"
        ],
        "summary": "Predict Food Class",
        "description": "Run the image classification model on an uploaded image and return the top predicted labels with their probabilities.\n\nThis endpoint accepts a multipart/form-data POST with a file field named 'image'.",
        "operationId": "predict_endpoint_predict_post",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/Body_predict_endpoint_predict_post"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PredictionResponse"
                }
              }
            }
          },
          "400": {
            "description": "File must be an image",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/src__backend__routers__predict__ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Prediction failed",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/src__backend__routers__predict__ErrorResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/predict/explain": {
      "post": {
        "tags": [
          "prediction"
        ],
        "summary": "Generate Attention Heatmap",
        "description": "Creates an attention-based visualization showing which parts of the image the model focuses on for prediction.",
        "operationId": "explain_prediction_predict_explain_post",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/Body_explain_prediction_predict_explain_post"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ExplainResponse"
                }
              }
            }
          },
          "400": {
            "description": "File must be an image",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/src__backend__routers__predict__ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Explainability generation failed",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/src__backend__routers__predict__ErrorResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/llm/generate": {
      "post": {
        "tags": [
          "llm"
        ],
        "summary": "Generate text from prompt",
        "description": "Generate text with the configured Ollama model.\n\nPOST a JSON body with `prompt`, optional `temperature` and `max_tokens`.\nReturns the generated text and the model used.",
        "operationId": "generate_text_llm_generate_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LLMRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LLMResponse"
                }
              }
            }
          },
          "503": {
            "description": "Ollama not running or model missing",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/src__backend__routers__llm__ErrorResponse"
                }
              }
            }
          },
          "504": {
            "description": "Ollama timeout",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/src__backend__routers__llm__ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/src__backend__routers__llm__ErrorResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/llm/health": {
      "get": {
        "tags": [
          "llm"
        ],
        "summary": "Ollama service health",
        "description": "Check whether the local Ollama service is reachable and return a list of available models plus the configured model name.\n\nReturns a JSON object with `status`, `ollama_url`, `available_models` and `configured_model`.",
        "operationId": "check_ollama_health_llm_health_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LLMHealthResponse"
                }
              }
            }
          },
          "503": {
            "description": "Ollama not running",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/src__backend__routers__llm__ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/src__backend__routers__llm__ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/dashboard": {
      "get": {
        "summary": "Metrics Dashboard",
        "description": "Enhanced visual dashboard for Prometheus metrics with charts.\nIncludes Chart.js for visualizations.",
        "operationId": "metrics_dashboard_dashboard_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "text/html": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/": {
      "get": {
        "summary": "Read Root",
        "operationId": "read_root__get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/StatusResponse"
                }
              }
            }
          }
        }
      }
    },
    "/health": {
      "get": {
        "summary": "Health Check",
        "description": "Lightweight health endpoint for container orchestration.\n\nReturns 200 OK when the app is up and routers are registered.",
        "operationId": "health_check_health_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/StatusResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Body_explain_prediction_predict_explain_post": {
        "properties": {
          "image": {
            "type": "string",
            "format": "binary",
            "title": "Image"
          }
        },
        "type": "object",
        "required": [
          "image"
        ],
        "title": "Body_explain_prediction_predict_explain_post"
      },
      "Body_predict_endpoint_predict_post": {
        "properties": {
          "image": {
            "type": "string",
            "format": "binary",
            "title": "Image"
          }
        },
        "type": "object",
        "required": [
          "image"
        ],
        "title": "Body_predict_endpoint_predict_post"
      },
      "ExplainResponse": {
        "properties": {
          "predicted_class": {
            "type": "string",
            "title": "Predicted Class",
            "examples": [
              "guacamole"
            ]
          },
          "confidence": {
            "type": "number",
            "maximum": 1.0,
            "minimum": 0.0,
            "title": "Confidence",
            "examples": [
              0.9968
            ]
          },
          "attention_map": {
            "type": "string",
            "title": "Attention Map",
            "description": "Base64-encoded PNG",
            "examples": [
              "iVBORw0KGgoAAAANS..."
            ]
          },
          "num_layers": {
            "type": "integer",
            "title": "Num Layers",
            "examples": [
              12
            ]
          },
          "num_heads": {
            "type": "integer",
            "title": "Num Heads",
            "examples": [
              12
            ]
          },
          "grid_size": {
            "type": "string",
            "title": "Grid Size",
            "examples": [
              "13x15"
            ]
          },
          "filename": {
            "type": "string",
            "title": "Filename",
            "examples": [
              "food.jpg"
            ]
          }
        },
        "type": "object",
        "required": [
          "predicted_class",
          "confidence",
          "attention_map",
          "num_layers",
          "num_heads",
          "grid_size",
          "filename"
        ],
        "title": "ExplainResponse",
        "example": {
          "attention_map": "iVBORw0KGgoAAAANS...",
          "confidence": 0.9968,
          "filename": "food.jpg",
          "grid_size": "13x15",
          "num_heads": 12,
          "num_layers": 12,
          "predicted_class": "guacamole"
        }
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "LLMHealthResponse": {
        "properties": {
          "status": {
            "type": "string",
            "title": "Status",
            "examples": [
              "healthy"
            ]
          },
          "ollama_url": {
            "type": "string",
            "title": "Ollama Url",
            "examples": [
              "http://localhost:11434"
            ]
          },
          "available_models": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "title": "Available Models",
            "examples": [
              [
                "gemma3:270m",
                "llama3.1:8b"
              ]
            ]
          },
          "configured_model": {
            "type": "string",
            "title": "Configured Model",
            "examples": [
              "gemma3:270m"
            ]
          }
        },
        "type": "object",
        "required": [
          "status",
          "ollama_url",
          "available_models",
          "configured_model"
        ],
        "title": "LLMHealthResponse",
        "example": {
          "available_models": [
            "gemma3:270m",
            "llama3.1:8b"
          ],
          "configured_model": "gemma3:270m",
          "ollama_url": "http://localhost:11434",
          "status": "healthy"
        }
      },
      "LLMRequest": {
        "properties": {
          "prompt": {
            "type": "string",
            "title": "Prompt",
            "examples": [
              "Write a haiku about masala dosa."
            ]
          },
          "temperature": {
            "type": "number",
            "maximum": 2.0,
            "minimum": 0.0,
            "title": "Temperature",
            "default": 0.7,
            "examples": [
              0.7
            ]
          },
          "max_tokens": {
            "type": "integer",
            "maximum": 4096.0,
            "minimum": 1.0,
            "title": "Max Tokens",
            "default": 500,
            "examples": [
              200
            ]
          }
        },
        "type": "object",
        "required": [
          "prompt"
        ],
        "title": "LLMRequest",
        "description": "Request model for LLM generation.",
        "example": {
          "max_tokens": 200,
          "prompt": "Write a haiku about masala dosa.",
          "temperature": 0.7
        }
      },
      "LLMResponse": {
        "properties": {
          "response": {
            "type": "string",
            "title": "Response",
            "examples": [
              "Golden crisp dosa,\nSpiced potato dreams within,\nCurry-scented dawn."
            ]
          },
          "model": {
            "type": "string",
            "title": "Model",
            "examples": [
              "gemma3:270m"
            ]
          },
          "done": {
            "type": "boolean",
            "title": "Done",
            "default": false,
            "examples": [
              true
            ]
          }
        },
        "type": "object",
        "required": [
          "response",
          "model"
        ],
        "title": "LLMResponse",
        "description": "Response model for LLM generation.",
        "example": {
          "done": true,
          "model": "gemma3:270m",
          "response": "Golden crisp dosa,\nSpiced potato dreams within,\nCurry-scented dawn."
        }
      },
      "PredictionResponse": {
        "properties": {
          "predictions": {
            "additionalProperties": {
              "type": "number"
            },
            "type": "object",
            "title": "Predictions"
          },
          "filename": {
            "type": "string",
            "title": "Filename"
          }
        },
        "type": "object",
        "required": [
          "predictions",
          "filename"
        ],
        "title": "PredictionResponse",
        "example": {
          "filename": "food.jpg",
          "predictions": {
            "guacamole": 0.996,
            "nachos": 0.001,
            "salsa": 0.002
          }
        }
      },
      "StatusResponse": {
        "properties": {
          "status": {
            "type": "string",
            "title": "Status"
          }
        },
        "type": "object",
        "required": [
          "status"
        ],
        "title": "StatusResponse",
        "example": {
          "status": "ok"
        }
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      },
      "src__backend__routers__llm__ErrorResponse": {
        "properties": {
          "detail": {
            "type": "string",
            "title": "Detail",
            "examples": [
              "Ollama service is not running. Please start Ollama first."
            ]
          }
        },
        "type": "object",
        "required": [
          "detail"
        ],
        "title": "ErrorResponse"
      },
      "src__backend__routers__predict__ErrorResponse": {
        "properties": {
          "detail": {
            "type": "string",
            "title": "Detail",
            "examples": [
              "File must be an image"
            ]
          }
        },
        "type": "object",
        "required": [
          "detail"
        ],
        "title": "ErrorResponse"
      }
    }
  }
}